{
  "name": "remote-mower",
  "version": "0.0.1",
  "private": true,
  "author": "Tarbadev",
  "repository": "https://github.com/tarbadev/remote-mower",
  "description": "Desktop application for Remote Mower using the husqvarna API",
  "license": "ISC",
  "homepage": "./",
  "main": "src/start.js",
  "scripts": {
    "electron": "NODE_ENV=dev electron .",
    "start": "BROWSER=none react-scripts start",
    "build": "react-scripts build",
    "test": "jest",
    "test:watch": "npm run test -- --watchAll",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "electron": "^8.2.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "jest": "^24.9.0",
    "jest-enzyme": "^7.1.2",
    "jest-junit": "^10.0.0",
    "react-scripts": "3.4.1"
  },
  "dependencies": {
    "@material-ui/core": "^4.9.7",
    "electron-is-dev": "^1.1.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "jest": {
    "resetMocks": true,
    "setupFilesAfterEnv": [
      "jest-enzyme"
    ],
    "testEnvironment": "enzyme",
    "testEnvironmentOptions": {
      "enzymeAdapter": "react16"
    },
    "reporters": [
      "default",
      "jest-junit"
    ],
    "moduleNameMapper": {
      "^src(.*)$": "<rootDir>/src$1"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
